{
	"name": "google-analytics-snippets-vscode",
	"displayName": "Google Analytics Snippets for VS Code",
	"description": "Google Analytics Snippet Pack helps you write global site tag (gtag.js) code that allows you to send event data to Google Analytics.",
	"version": "0.1.0",
	"publisher": "clydedsouza",
	"engines": {
		"vscode": "^1.28.0"
	},
	"license": "MIT",
	"preview": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/ClydeDz/google-analytics-snippets-vscode"
	},
	"icon": "icon.png",
	"keywords": [
		"Snippets",
		"Google Analytics",
		"Google",
		"VS Code",
		"Snippet Pack",
		"JavaScript",
		"HTML"
	],
	"categories": [
		"Snippets"
	],
	"contributes": {
		"snippets": [ 
			{
				"language": "javascript",
				"path": "./snippets/javascript.json"
			},
			{
				"language": "html",
				"path": "./snippets/html.json"
			}
		]
	},
	"devDependencies": {
		"chai": "^4.2.0",
		"mocha": "^6.1.4",
		"mocha-junit-reporter": "^1.22.0"
	},
	"scripts": {
		"test": "mocha test/test.js --reporter mocha-junit-reporter --timeout 10000 --exit"
	}
}
